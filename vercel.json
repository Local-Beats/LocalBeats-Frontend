{
  "version": 2,
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/main.js",
      "dest": "/main.js"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/react-logo.svg",
      "dest": "/react-logo.svg"
    },
    {
      "src": "/spongebob-404.webp",
      "dest": "/spongebob-404.webp"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}